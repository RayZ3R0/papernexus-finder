<script>
  function isMobile() {
    return ('ontouchstart' in window || navigator.maxTouchPoints > 0 || navigator.msMaxTouchPoints > 0)
        && window.innerWidth < 768;
  }
  
  function isSafari() {
    return /^((?!chrome|android).)*safari/i.test(navigator.userAgent);
  }

  // Parse URL parameters
  function getURLParameter(name) {
    name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
    var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
    var results = regex.exec(location.search);
    return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
  }

  document.addEventListener('DOMContentLoaded', function() {
    // Extract the 'pdf' parameter from the URL
    var pdfUrl = getURLParameter('pdf');

    if ((isMobile() || isSafari()) && pdfUrl) {
      window.location.href = pdfUrl; // Redirect mobile or Safari users to the direct PDF link
    } else if (pdfUrl) {
      var iframe = document.querySelector('.iframe-wrapper iframe');
      if (iframe) { // Check if iframe exists
        iframe.setAttribute('src', pdfUrl); // If the user is not on mobile or Safari and there's a 'pdf' parameter, set it as the iframe src for desktop users
      }
    }
  });
</script>

